{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-undef": "error",
		"no-console": "off",
		"no-var": "error",
		"prefer-const": "warn",
		"eqeqeq": "error",
		"no-unreachable": "error",
		"no-debugger": "warn"
	}
}
